{
  "name": "open-listings",
  "version": "1.0.0",
  "description": "A performant listings website with maps and tags",
  "main": "index.js",
  "scripts": {
    "lint": "eslint 'src/**/*.js'",
    "lint-fix": "eslint --fix 'src/**/*.js'",
    "pretty": "prettier --ignore-unknown --write src/**/*.js",
    "prepare": "husky install",
    "precommit": "yarn run lint-fix && yarn run pretty",
    "download-assets": "curl https://raw.githubusercontent.com/stamparm/ipsum/master/ipsum.txt > data/raw/ipsum.txt",
    "flow": "flow",
    "flow:untyped": "flow-typed create-stub",
    "flow:install": "flow-typed install",
    "build": "flow-remove-types src/ -d dist/",
    "build_0": "flow-remove-types src/ -d dist/",
    "install:linux": "sudo apt-get install -y build-essential dos2unix golang-go && yarn run download-assets && yarn && cd client && yarn && yarn run build && cd .. && curl -sf https://gobinaries.com/tj/node-prune | sh && node-prune",
    "start_0": "yarn run build && cross-env APP_NAME=\"Les periodiques\" node --trace-deprecation dist/server",
    "start_1": "yarn run build && cross-env APP_NAME=open_listings_ar node --trace-deprecation dist/server",
    "start_2": "yarn run build && cross-env APP_NAME=open_listings_en node --trace-deprecation dist/server",
    "start:dev": "cross-env DEBUG=app:* APP_NAME=open_listings_ar nodemon dist/server",
    "stop": "kill $(ps aux | grep 'open_listings*' | awk '{print $2}')",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:e2e": "NODE_ENV=api tap && ava",
    "//postinstall": "node-prune"
  },
  "type": "module",
  "keywords": [
    "Node.js",
    "JavaScript",
    "MongoDB",
    "Maps",
    "web-app"
  ],
  "license": "",
  "engines": {
    "node": ">=18.18.0"
  },

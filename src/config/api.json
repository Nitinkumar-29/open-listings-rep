{
   "ENVIRONMENT": "api",
   "NODE_PORT": 3003,
   "PIPELINE_KEYWORDS_SECONDS": 86400,
   "MONGODB_URI": "mongodb://{dbUserName}:{dbUserPWD}@0.0.0.0:27017/{dbName}?directConnection=true",
   "REDIS_HOST": "127.0.0.1",
   "APIHost": "http://localhost:3003",
   "COOKIE_NAME": "open-listings-login",
   "PING_LIMITER": {
      "max": 100,
      "timeWindow": "1 minute"
   },
   "SMTP": {
      "host": "smtp.office365.com",
      "from": "{ADMIN_EMAIL}",
      "port": 587,
      "tls": {
         "ciphers": "SSLv3"
      },
      "secure": false,
      "auth": {
         "user": "{ADMIN_EMAIL}",
         "pass": "{ADMIN_PASS}"
      }
   },
   "features": {
      "geo": false
   },
   "": "// careful, this should never change ! This is how data is indexed forever",
   "TAG_HARD_SIZE_LIMIT": 60,
   "TYPESENSE": {
      "nodes": [
         {
            "host": "0.0.0.0",
            "port": 8108,
            "protocol": "http"
         }
      ],
      "apiKey": "{TYPESENSE_API_KEY}",
      "connectionTimeoutSeconds": 2
   }
}